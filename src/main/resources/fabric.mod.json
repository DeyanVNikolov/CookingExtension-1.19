    "sources": "https://github.com/DeyanVNikolov/CookingExtension-1.19",
    "issues": "https://github.com/DeyanVNikolov/CookingExtension-1.19/issues"
  },

  "license": "MIT License",
  "icon": "assets/modid/icon.png",

  "environment": "*",
